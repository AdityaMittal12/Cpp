//WAP to connt the number of elements in given array greater than a given number'x'

#include <iostream>
using namespace std;
int main() {
    int arr[] = {1,3,0,10,2,5,6};
    int n = sizeof(arr)/4;
    
    int x;
    cout<<"Enter the number : ";
    cin>>x;
    
    int count = 0;
    
    for(int i=0; i<n; i++){
        if(x<arr[i]) count++;
    }
    cout<<count;
}
