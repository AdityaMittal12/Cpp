//WAP to check if given number is prime or composite

#include<iostream>
using namespace std;
int main(){
    int n;
    cout<<"Enter n : ";
    cin>>n;
    bool x = true;         //assume no. is prime
    for(int i=2;i<=n/2;i++){
        if (n%i==0){
            x = false;    // assumption is contradicted, so no. is composite
            break;
        }
    }
    if (x=true) {cout<<"Prime no.";}
    else cout<< "Composite no.";
}
