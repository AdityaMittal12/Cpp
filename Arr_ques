//Given an integer n. Create an array containing squares of all natural numbers till n and print the elements of the array.

#include <iostream>
using namespace std;
int main() {
    int n;
    cout<<"Enter n : ";
    cin>>n;
    int arr[n];               //declaring array
    for(int i=1;i<=n;i++){   //initializing array acc. to ques                
        arr[i-1] = i*i;
    }
    cout<<endl;
    for(int i=0; i<n ; i++){        //printing array
        cout<<arr[i]<<" ";
    }
}
