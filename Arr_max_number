//WAP to find the maximum value out of all the elements in the array

#include <iostream>
using namespace std;
int main() {
    int arr[] = {1,5,6,4,3};
    int n = sizeof(arr)/4;
    int mx = arr[0];
    for(int i=1;i<n;i++){
        // if (mx < arr[i]) mx =arr[i];
        mx = max(mx,arr[i]);
    }
    cout<<mx;
}
