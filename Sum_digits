//WAP to print sum of digits of the given number

#include <iostream>
using namespace std;
int main() {
    int n;
    cout<<"Enter n : ";
    cin>>n;
    int sum=0;
    while(n!=0){
        sum = sum + n%10;      // x%10 gives the last digit.
        n = n/10;              // x/10 gives all digits except last digit.
    }
    cout<<sum;
}
